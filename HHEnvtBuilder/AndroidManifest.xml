<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hubee.hh.launchertablet.envtbuilder"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".controllers.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".controllers.WelcomeActivity"
            android:label="@string/title_activity_welcome" />
        <activity
            android:name=".controllers.SetupAccountActivity"
            android:label="@string/title_activity_welcome" />
         <activity
            android:name=".controllers.SetupProfileActivity"
            android:label="@string/title_activity_welcome" />
    </application>

</manifest>